<?php

$student=array(array("Name"=>"Ankit","RollNo"=>"121"),
array("Name"=>"Ram","RollNo"=>"1234"),
array("Nmae"=>"Mohit","RollNo"=>"12345"),
array("Name"=>"Ajay","RollNo"=>"2365"),
array("Name"=>"Ranjeet","RollNo"=>56789));
// echo "<pre>";
// print_r($student);
echo "<table border='5px;'>";
echo "<tr>";
foreach($student[0] as $key=>$value){
    echo "<th>";
    echo $key;
    echo "</th>"; 
}    
    foreach($student as $row){
        echo "<tr>";
        foreach($row as $value)
        {
            echo "<td>";
            echo $value;
            echo "</td>";
        }
        echo "</tr>";
        
    }
    

echo "</tr>";
echo "</table>";
?>
